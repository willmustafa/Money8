generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  fullName String
  email String @unique
  password String
}

model Transaction {
  id String @id @default(uuid())
  description String
  date DateTime @db.Date
  value Float
  paid Boolean
  transferedTo String
}

model Institution {
  id String @id @default(uuid())
  name String @unique
  color String
  icon String
}

model Ignore {
  id String @id @default(uuid())
  name String
}